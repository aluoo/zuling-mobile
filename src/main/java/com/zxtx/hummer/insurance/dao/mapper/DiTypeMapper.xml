<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zxtx.hummer.insurance.dao.mapper.DiTypeMapper">

    <select id="selectByParam" resultType="com.zxtx.hummer.insurance.vo.DiTypeVO">
        SELECT
        *
        FROM di_type
        <where>
            1=1
            <if test="name != null and name != '' ">
                and name LIKE concat(#{name}, '%')
            </if>
            <if test="status != null">
                and status = #{status}
            </if>
            <if test="packageAble != null">
                and package_able = #{packageAble}
            </if>
        </where>
        order by create_time desc
    </select>

</mapper>
